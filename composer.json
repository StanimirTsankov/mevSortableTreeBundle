{
    "name": "mikeevstropov/sortable-tree-bundle",
    "type": "symfony-bundle",
    "description": "Offers a sortable feature for your Symfony2/3 admin tree listing",
    "keywords": ["sortable","tree","admin","sonata"],
    "homepage": "https://github.com/mikeevstropov/mevSortableTreeBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Evstropov",
            "email": "mike.evstropov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": ">=2.1",
        "symfony/form": ">=2.1"
    },
    "require-dev": {
        "gedmo/doctrine-extensions": ">=2.2"
    },
    "suggest": {
        "sonata-project/admin-bundle": "Can be easily integrated with Sonata Admin bundle",
        "stof/doctrine-extensions-bundle": "1.1.*@dev"
    },
    "autoload": {
        "psr-0": { "Mev\\SortableTreeBundle": "" }
    },
    "target-dir": "Mev/SortableTreeBundle",
	"extra": {
    }
}
